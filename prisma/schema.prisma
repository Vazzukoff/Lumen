generator client {
  provider = "prisma-client-js"
  binaryTargets   = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  name       String
  email      String   @unique
  password   String
  age        Int?
  gender     String?
  country    String?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
}